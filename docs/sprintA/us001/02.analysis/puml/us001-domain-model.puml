@startuml
skinparam monochrome true
skinparam packageStyle rectangle
skinparam shadowing false

skinparam classAttributeIconSize 0

hide circle
hide methods

class Organization {
    name
    vatNumber
    website
    phoneNumber
    email
}

class Skills {
    name
}

class HRM {
    name
    email
    position
    phone
}

Organization "1" - "1" HRM: has >
HRM "1" -- "0..*" Skills : regist >

@enduml